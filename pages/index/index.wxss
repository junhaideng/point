/**index.wxss**/
.container {
  background-color: #fcfcfc;
  height: 100%;
}

.total {
  font-size: 20px;
  position: fixed;
  top: 0;
  text-align: center;
  width: 100%;
  background-color: #ffffff;
  z-index: 10000;
  margin-bottom: 5px;
}

.items {
  margin-top: 40px;
}
.item {
  width: 100%;
}

.van-swipe-cell__left,
.van-swipe-cell__right {
  display: inline-block;
  width: 65px;
  height: 44px;
  font-size: 15px;
  line-height: 44px;
  color: #fff;
  text-align: center;
  background-color: #ee0a24;
}